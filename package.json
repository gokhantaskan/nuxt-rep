{
  "name": "nuxt-app-template",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.15.0",
  "pnpm": {
    "overrides": {
      "@vercel/nft": "^0.27.7",
      "nitropack": "~2.10.4",
      "ofetch": "^1.4.1"
    }
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "pnpm dev:db && pnpm dev:client",
    "dev:client": "nuxt dev",
    "dev:db": "chmod +x ./dev-setup.sh && ./dev-setup.sh",
    "dev:clean": "nuxt cleanup && pnpm dev",
    "dev:prod": "env-cmd -f .env.production --verbose nuxt dev",
    "generate": "nuxt generate",
    "preview": "NITRO_SSL_CERT=$(cat localhost+4.pem) NITRO_SSL_KEY=$(cat localhost+4-key.pem) nuxt preview",
    "preview:prod": "env-cmd -f .env.production --verbose pnpm preview",
    "type-check": "nuxt typecheck",
    "lint": "eslint . --fix",
    "lint:styles": "stylelint '**/*.{vue,css,scss}' --fix",
    "format": "prettier '**/*.{css,scss,md,yml,yaml,json}' -l -u --write",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "clean": "rimraf --glob **/dist **/.nuxt **/.output **/node_modules/.vite **/node_modules/.cache **/inspect",
    "act": "pnpm --package act dlx act",
    "act:mac": "pnpm --package act dlx act --container-architecture linux/amd64",
    "clean:all": "pnpm clean && rimraf --glob **/node_modules",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@nuxt/eslint": "^0.7.2",
    "@nuxt/icon": "^1.9.1",
    "@nuxt/image": "^1.8.1",
    "@nuxt/scripts": "^0.9.5",
    "@nuxtjs/i18n": "^9.1.1",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@pinia/nuxt": "^0.9.0",
    "@sentry/nuxt": "^8.42.0",
    "@vee-validate/nuxt": "^4.14.7",
    "@vee-validate/zod": "^4.14.7",
    "dayjs": "^1.11.13",
    "dayjs-nuxt": "^2.1.11",
    "drizzle-orm": "^0.37.0",
    "drizzle-zod": "^0.5.1",
    "lodash-es": "^4.17.21",
    "nuxt": "^3.14.1592",
    "pg": "^8.13.1",
    "pinia": "^2.3.0",
    "ua-parser-js": "^2.0.0",
    "validator": "^13.12.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nuxtjs/stylelint-module": "^5.2.0",
    "@types/lodash-es": "^4.17.12",
    "@types/pg": "^8.11.10",
    "@types/validator": "^13.12.2",
    "@vueuse/core": "^12.0.0",
    "@vueuse/nuxt": "^12.0.0",
    "drizzle-kit": "^0.29.1",
    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "sass": "^1.82.0",
    "stylelint": "^16.11.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-scss": "^6.10.0",
    "tailwindcss": "^3.4.16",
    "typescript": "5.6.3",
    "vue-tsc": "^2.1.10"
  }
}
